{
  "name": "bowling-score-tracker",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "start:dev": "vite",
    "start:container": "chmod +x ./${pwd}/startup.sh && ./${pwd}/startup.sh",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "typecheck": "tsc --noEmit",
    "lint": "yarn eslint && yarn stylelint",
    "eslint": "eslint . --cache",
    "stylelint": "stylelint \"**/*.css\" --cache",
    "prettier": "prettier --check \"**/*.{ts,tsx}\"",
    "prettier:write": "prettier --write \"**/*.{ts,tsx}\"",
    "vitest": "vitest run",
    "vitest:watch": "vitest",
    "vitest:coverage": "vitest --coverage --watch=false",
    "test": "yarn typecheck && yarn prettier && yarn lint && yarn vitest:coverage && yarn build",
    "storybook": "storybook dev -p 6006",
    "storybook:build": "storybook build",
    "prepare": "husky"
  },
  "dependencies": {
    "@apollo/client": "^3.13.1",
    "@graphql-typed-document-node/core": "^3.2.0",
    "@mantine/core": "^7.17.1",
    "@mantine/dates": "^7.17.1",
    "@mantine/form": "^7.17.1",
    "@mantine/hooks": "^7.17.1",
    "@mui/material": "^6.4.7",
    "@storybook/preview-api": "^8.6.6",
    "@tabler/icons-react": "^3.30.0",
    "dayjs": "^1.11.13",
    "graphql": "^16.10.0",
    "mantine-form-zod-resolver": "^1.1.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^7.2.0",
    "zod": "^3.24.2"
  },
  "devDependencies": {
    "@eslint/js": "^9.21.0",
    "@graphql-codegen/cli": "^5.0.5",
    "@graphql-codegen/client-preset": "^4.6.4",
    "@graphql-codegen/typescript-react-apollo": "^4.3.2",
    "@ianvs/prettier-plugin-sort-imports": "^4.4.1",
    "@storybook/addon-essentials": "^8.6.6",
    "@storybook/addon-interactions": "^8.6.6",
    "@storybook/addon-links": "^8.6.6",
    "@storybook/blocks": "^8.6.6",
    "@storybook/react": "^8.6.6",
    "@storybook/react-vite": "^8.6.6",
    "@storybook/testing-library": "^0.2.2",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.2.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/node": "^20.17.23",
    "@types/react": "^18.3.18",
    "@types/react-dom": "^18.3.5",
    "@vitejs/plugin-react": "^4.3.4",
    "@vitest/coverage-v8": "^2.1.9",
    "eslint": "^9.21.0",
    "eslint-config-mantine": "^4.0.3",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-react": "^7.37.4",
    "husky": "^9.1.7",
    "identity-obj-proxy": "^3.0.0",
    "jsdom": "^25.0.1",
    "postcss": "^8.5.3",
    "postcss-preset-mantine": "1.17.0",
    "postcss-simple-vars": "^7.0.1",
    "prettier": "^3.5.3",
    "prop-types": "^15.8.1",
    "storybook": "^8.6.6",
    "storybook-dark-mode": "^4.0.2",
    "stylelint": "^16.15.0",
    "stylelint-config-standard-scss": "^13.1.0",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.25.0",
    "vite": "^6.2.0",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^2.1.9"
  },
  "jest": {
    "testEnvironment": "jsdom",
    "moduleFileExtensions": [
      "js",
      "jsx",
      "ts",
      "tsx"
    ],
    "transform": {
      "^.+\\.(js|jsx|ts|tsx)$": "babel-jest"
    },
    "moduleNameMapper": {
      "^.+\\.(css|less|scss)$": "identity-obj-proxy"
    }
  }
}